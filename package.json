{
  "name": "agro-app",
  "version": "1.0.0",
  "description": "AgroApp's application.",
  "main": "scripts/main.js",
  "scripts": {
    "start": "npm run build:client && node bin/www.js",
    "start:dev": "npm run build:client && nodemon -r dotenv/config bin/www dotenv_config_path=env/dev.env || true",
    "build:styles": "node scripts/styles",
    "build:bundle": "node scripts/build_bundle",
    "build:libs": "node scripts/build_libs",
    "build:client": "npm run build:libs && npm run build:bundle && npm run build:styles",
    "test:unit": "mocha --require test/unit/options.js test/unit/**/*.spec.js || true",
    "test:integration": "mocha --require test/integration/options.js test/integration/*.spec.js || true",
    "test": "npm run test:unit && npm run test:integration"
  },
  "author": "Murano Design <contato@muranodesign.com.br>",
  "contributors": [
    "Lucas Tavares <lucastavaresdarocha@gmail.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.0.2",
    "concat-files": "^0.1.1",
    "cssmin": "^0.4.3",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-sass": "^2.2.1",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.2",
    "uglify-js": "^3.0.27"
  },
  "dependencies": {
    "Base64": "^1.0.1",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "^1.6.4",
    "angular-cookies": "^1.6.5",
    "angular-material": "^1.1.4",
    "angular-messages": "^1.6.5",
    "angular-route": "^1.6.4",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "mysql2": "^1.3.5",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pm2": "^2.5.0",
    "sequelize": "^4.2.1"
  }
}
