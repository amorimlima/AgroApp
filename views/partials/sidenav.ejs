<md-sidenav 
  layout="column"
  md-is-open="sidenav.visible"
  md-component-id="main_menu">

  <md-toolbar
    flex="15"
    layout="column"
    layout-align="end center">
  </md-toolbar>

  <md-list flex="75">
    <md-list-item 
      ng-repeat="menuItem in sidenav.items"
      ng-click="sidenav.toggle()"
      ng-href="{{menuItem.href}}">

      <span>
        <md-icon md-font-set="material-icons">{{menuItem.icon}}</md-icon>
        {{menuItem.name}}
      </span>
      <md-divider></md-divider>
    </md-list-item>
  </md-list>

  <div
    flex="10"
    layout="column"
    layout-align="end center">

    <md-button
      md-ripple-size="full"
      ng-disabled="form.cpf.$error.em_uso || form.$invalid"
      ng-click="sair()">

      Sair
    </md-button>
  </div>
</md-sidenav>